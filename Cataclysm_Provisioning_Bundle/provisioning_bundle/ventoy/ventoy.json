{
  "control": [
    {
      "VTOY_DEFAULT_MENU_MODE": "1"
    },
    {
      "VTOY_FILT_DOT_UNDERSCORE_FILE": "1"
    }
  ],
  "auto_install": [
    {
      "image": "/isos/Windows11_Pro.iso",
      "template": "/windows/Autounattend.xml",
      "replace": [
        {
          "iso_file": ".*Windows.*11.*\\.iso$"
        }
      ]
    },
    {
      "image": "/isos/ubuntu-24.04.1-live-server-amd64.iso",
      "template": "/linux/ubuntu-autoinstall/user-data",
      "replace": [
        {
          "iso_file": ".*ubuntu-.*server-.*\\.iso$"
        }
      ]
    }
  ]
}